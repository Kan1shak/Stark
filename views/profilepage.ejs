<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Profile</title>

    
    <link rel="stylesheet" href="css/profile.css">
    <link rel="stylesheet" href="css/profileres.css">

    
    <link href="https://cdn.jsdelivr.net/npm/remixicon@3.5.0/fonts/remixicon.css" rel="stylesheet"></head>

<body>
   
    <span class="main_bg"></span>


    <div class="container">

        
        <header>
            <div class="brandLogo">
                <figure><img src="../images/stark.png" alt="logo" width="50px" height="40px"></figure>
                
            </div>
        </header>


       
        <section class="userProfile card">
            <div class="profile">
                <figure><img src="../images/avatar7.png" alt="profile" width="250px" height="250px"></figure>
            </div>
        </section>


        <!-- /////////////// Work & Skills Section /////////////-->
        <section class="work_skills card">

            <!-- /////////// Work Contaienr //////// -->
            <div class="work">
                <h1 class="heading">work</h1>
                <div class="primary">
                    <h1>Stark</h1>
                    
                    <p>Vit Bhopal <br> Sehore, Bhopal</p>
                    
                </div>

                
            </div>

            <!-- //////// Skills //////////// -->
            <div class="skills">
                <h1 class="heading">Skills</h1>
                <ul>
                    <li style="--i:0">Android</li>
                    <li style="--i:1">Web-Design</li>
                    <li style="--i:2">UI/UX</li>
                    <li style="--i:3">Video Editing</li>
                </ul>
            </div>
        </section>


        <!-- ////////////USER DETAILS/////////////// -->
        <section class="userDetails card">
            <div class="userName">
                <h1 class="name">Rishabh Kumar</h1>
                <div class="map">
                    <i class="ri-map-pin-fill ri"></i>
                    
                    
                </div>
                <p>Web Developer</p>
                <p1>I'm a student currently studying <span>@VIT</span>.</p1>
            </div>

          
        </section>


        <!-- /////////////////ABOUT/////////////////// -->
        <section class="timeline_about card">
            <div class="tabs">
                <ul>
                    

                    <li class="about active">
                        <i class="ri-user-3-fill ri"></i>
                        <span>About</span>
                    </li>
                </ul>
            </div>

            <div class="contact_Info">
                <h1 class="heading">Contact Information</h1>
                <ul>
                    <li class="phone">
                        <h1 class="label">Phone:</h1>
                        <span class="info">1234566789</span>
                    </li>

                    <li class="address">
                        <h1 class="label">Address:</h1>
                        <span class="info">Vit Bhopal, <br> Sehore, Bhopal-466114</span>
                    </li>

                    <li class="email">
                        <h1 class="label">E-mail:</h1>
                        <span class="info">rishabh@gmail.com</span>
                    </li>

                    <li class="site">
                        <h1 class="label">Site:</h1>
                        <span class="info">https://kan1shak.github.io/Stark/</span>
                    </li>
                </ul>
            </div>

            <div class="basic_info">
                <h1 class="heading">Basic Information</h1>
                <ul>
                    <li class="birthday">
                        <h1 class="label">Birthday:</h1>
                        <span class="info">Dec 25, 2000</span>
                    </li>

                    <li class="sex">
                        <h1 class="label">Gender:</h1>
                        <span class="info">Male</span>
                    </li>
                </ul>
            </div>
        </section>
    </div>

</body>

</html>